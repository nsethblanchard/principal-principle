<%= form_for(@user, url:'/signup') do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email, required:true %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.label :school %>
    <%= f.text_field :school %>
    <br>
    <br>
    
    <%= f.submit "Create Your New School Admin Experience" %>

<% end %>