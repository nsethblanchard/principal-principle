<h1> Assignment Index Page </h1>
<%= flash[:message] if flash[:message] %>

<% if @assignments == [] %>
    <h2>No current assignments. </h2>
<% else %>

    <% @assignments.each do |assign| %>
        <%= assign.title if assign.title %>
        <%= assign.content if assign.content %>    
    <br>
    <% end %>
<% end %>